@model List<AppUser>
@{
    ViewData["Title"] = "AddThisUserToRole";
}

<h1>Pick  for the user</h1>

@foreach (var item in Model)
{
    <table>
        <tr>
            <td type="button" class=" btn-link ">

                <div class="btn-btn">
                    @Html.ActionLink(item.UserName, "AddUserToRoleSave", "Account", new { userId = item.UserName, roleId = item.Id })
          </div>  </td>
        </tr>
    </table>
   
}